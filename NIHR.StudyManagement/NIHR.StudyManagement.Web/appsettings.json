{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cognito": {
    "ClientId": "<set Cognito app client id>",
    "ClientSecret": "<set Cognito app client secret>",
    "MetadataAddress": "<set Cognito user pool domain .well-known configuration endpoint>",
    "ResponseType": "code",
    "AppSignOutUrl": "/",
    "NihrIdentityProviderName": "NIHRIdentityGateway",
    "Domain": "<set Cognito user pool domain>",
    "RedirectUrl": "set to root app domain /signin-oidc",
    "Authority": "<set to cognito token endpoint>"
  }
}